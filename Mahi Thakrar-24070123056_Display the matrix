#include <iostream>
using namespace std;

int main() {
    int a1[3][3];
    int i, j;

    cout << "Enter elements of the 3x3 matrix:" << endl;

    // Input elements of matrix
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
            cin >> a1[i][j];
        }
    }

    // Displaying the matrix
    cout << "The matrix is:" << endl;
    for (i = 0; i < 3; i++) {
        for (j = 0; j < 3; j++) {
            cout << a1[i][j] << " ";
        }
        cout << endl;
    }

    return 0;
}



//Output:
//Enter elements of the 3x3 matrix:
//1 2 3 
//4 5 6
//7 8 9
//The matrix is:
//1 2 3 
//4 5 6 
//7 8 9 
